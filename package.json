  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --inline --progress --colors --hot --config ./webpack.dev.js",
    "build": "webpack --env=prod --progress --profile --colors"
  },
  "config": {
    "neutrino": {
      "devServer": {
        "port": 5000
      },
      "entry": {
        "react": [
          "react",
          "react-dom",
          "moment"
        ]
      },
      "module": {
        "rule": {
          "css": {
            "test": "\\.styl$",
            "loader": {
              "stylus": {
                "loader": "stylus-loader"
              }
            }
          }
        }
      }
    },
    "html": {
      "title": "POVO",
      "minify": false,
      "scripts": [
        "https://unpkg.com/classnames@^2.2",
        "https://unpkg.com/tether@^1.4"
      ]
    }
  }
