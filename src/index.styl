.tright
  text-align right

.bold
  font-weight 700

.fullw
  width 100%

.nopad
  padding 0

*:focus
  outline none

a
  color inherit
  text-decoration none

ul, body, html, p
  padding 0
  margin 0

ul
  list-style-type none

body
  font-family Arial, Helvetica, sans-serif
  text-rendering optimizeLegibility